# Database
DATABASE_URL="postgresql://..."
DIRECT_URL="postgresql://..."

# Supabase
NEXT_PUBLIC_SUPABASE_URL="https://..."
NEXT_PUBLIC_SUPABASE_ANON_KEY="eyJ..."
SUPABASE_SERVICE_ROLE_KEY="eyJ..."

# Callback URLs for n8n integration
# Production callback URL (your live site)
NEXT_PUBLIC_CALLBACK_BASE_URL_PRODUCTION=https://agency-hub-two.vercel.app

# Testing callback URL (MUST use ngrok for local development)
# IMPORTANT: n8n cannot reach localhost - you must use ngrok!
# 1. Install ngrok: brew install ngrok
# 2. Run: ngrok http 3001
# 3. Copy the https URL here (e.g., https://abc123.ngrok.io)
# 4. Restart your dev server after updating this value
# NEXT_PUBLIC_CALLBACK_BASE_URL_TESTING=https://your-ngrok-url.ngrok.io

# Auth Debug Mode (optional)
# Enable auth debug logging to troubleshoot authentication issues
# Set to 'true' to see detailed auth flow logs in browser console
NEXT_PUBLIC_AUTH_DEBUG=false

# Stripe
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=pk_test_...
STRIPE_SECRET_KEY=sk_test_...
STRIPE_WEBHOOK_SECRET=whsec_...

# Store Configuration
NEXT_PUBLIC_STORE_ENABLED=true
STRIPE_SUCCESS_URL=http://localhost:3001/store/success
STRIPE_CANCEL_URL=http://localhost:3001/store/cart

# Invoice Configuration
ENABLE_CONTRACTS=true
INVOICE_PREFIX=INV
INVOICE_STARTING_NUMBER=1000