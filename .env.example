# Supabase
NEXT_PUBLIC_SUPABASE_URL=https://[YOUR-PROJECT-REF].supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# Database (for Prisma)
DATABASE_URL=postgresql://postgres.[YOUR-PASSWORD]@aws-0-us-west-1.pooler.supabase.com:6543/postgres?pgbouncer=true
DIRECT_URL=postgresql://postgres.[YOUR-PASSWORD]@aws-0-us-west-1.pooler.supabase.com:5432/postgres

# App
NEXT_PUBLIC_APP_URL=http://localhost:3000

# Auth Debug Mode (optional)
# Enable auth debug logging to troubleshoot authentication issues
# Set to 'true' to see detailed auth flow logs in browser console
NEXT_PUBLIC_AUTH_DEBUG=false